{
  "apify": {
    "endpoint": "all"
  },
  "filters": {
    "users": [
      "fileUser1",
      "fileUser2"
    ],
    "keywords": [
      "fileKeyword1"
    ]
  },
  "outputs": {
    "dashboard": {
      "port": 8080
    }
  },
  "dedup": {
    "ttl": 30
  },
  "reconnect": {
    "initialDelay": 500,
    "maxDelay": 15000,
    "backoffMultiplier": 1.5,
    "maxAttempts": 5
  }
}